{
	"name": "canonical",
	"version": "0.8.0",
	"private": true,
	"scripts": {
		"dev": "vite",
		"dev:test": "VITE_USE_EMULATOR=true vite",
		"build": "vite build",
		"preview": "vite preview",
		"dev-local": "vite --host",
		"test": "vitest",
		"test:ui": "vitest --ui",
		"test:run": "vitest run",
		"test:invitation": "vitest run src/__tests__/integration/invitation-management.test.js",
		"test:user-management": "vitest run src/components/__tests__/UserManagement.test.js",
		"test:integration": "vitest run src/__tests__/integration/",
		"test:components": "vitest run src/components/__tests__/",
		"coverage": "vitest run --coverage",
		"emulators": "firebase emulators:start --import=./emulator-data --export-on-exit",
		"emulators:fresh": "firebase emulators:start",
		"emulators:ui": "firebase emulators:start --only firestore,auth,ui",
		"seed:invitations": "NODE_OPTIONS='--dns-result-order=ipv4first' node scripts/seed-emulator-data.js",
		"seed:clear": "node scripts/seed-emulator-data.js clear"
	},
	"dependencies": {
		"@mdi/font": "5.9.55",
