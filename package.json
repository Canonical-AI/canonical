		"@milkdown/plugin-diagram": "^7.7.0",
		"@milkdown/plugin-slash": "^7.5.0",
		"@milkdown/preset-commonmark": "^7.13.1",
		"@milkdown/theme-nord": "^7.13.1",
		"@milkdown/utils": "^7.13.1",
		"@milkdown/vue": "^7.5.0",
		"@prosemirror-adapter/vue": "^0.4.1",
		"@tailwindcss/typography": "^0.5.15",
		"dayjs": "^1.11.0",
		"dotenv": "^16.4.7",
		"firebase": "^11.0.0",
		"firebase-functions": "^6.0.0",
		"lodash": "^4.17.21",
		"marked": "^14.1.2",
		"mermaid": "^11.6.0",
		"pinia": "^3.0.3",
		"remark-directive": "^3.0.0",
		"three": "^0.170.0",
		"vite-plugin-vuetify": "^2.0.4",
		"vue": "^3.2.13",
		"vue-draggable-plus": "^0.5.3",
		"vue-gtag": "^2.0.1",
		"vue-router": "^4.0.13",
		"vuetify": "^3.7.0-beta.1",
		"vuex": "^4.0.2",
		"webfontloader": "^1.0.0"
	},
	"devDependencies": {
		"@babel/core": "^7.25.8",
		"@babel/eslint-parser": "^7.12.16",
